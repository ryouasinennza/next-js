    "@storybook/addon-links": "^6.4.0",
    "@storybook/react": "^6.4.0",
    "@types/node": "16.11.10",
    "@types/react": "17.0.37",
    "@types/styled-components": "^5.1.15",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^2.0.1",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.3.0",
