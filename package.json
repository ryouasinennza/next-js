{
  "name": "nextjs-starterkit",
  "private": true,
  "engines": {
    "node": "18.12.1",
    "pnpm": "7.18.0"
  },
  "volta": {
    "node": "18.12.1",
    "pnpm": "7.18.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "start": "next start",
    "sb": "start-storybook -p 6006",
    "sb-build": "build-storybook",
    "lint": "stylelint --fix './src/**/*.{ts,tsx}' & eslint --fix './src/**/*.{ts,tsx}' & prettier --write './src/**/*'",
    "tsc": "tsc",
    "test": "jest",
    "ncu-minor": "npx npm-check-updates -t minor",
    "ncu-minor-upgrade": "npx npm-check-updates -t minor --upgrade",
    "ncu-all": "npx npm-check-updates",
    "ncu-all-upgrade": "npx npm-check-updates --upgrade",
    "prepare": "husky install",
    "chromatic": "chromatic --project-token=CHROMATIC_PROJECT_TOKEN",
    "clean": "npm cache clean --force && rm -rf ./.next ./storybook-static ./out ./node_modules ./tsconfig.tsbuildinfo ./pnpm-lock.yaml",
    "pnpmupgrade": "brew upgrade pnpm"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --write"
    ]
  },
