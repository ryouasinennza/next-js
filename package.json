{
  "name": "nextjs-starterkit",
  "private": true,
  "engines": {
    "node": "16.17.1",
    "npm": "8.15.0"
  },
  "volta": {
    "node": "16.17.1",
    "npm": "8.15.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint ",
    "start": "next start",
    "sb": "start-storybook -p 6006",
    "sb-build": "build-storybook",
    "stylelint": "stylelint --fix './src/**/*.tsx'",
    "eslint": "eslint --fix './src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*'",
    "tsc": "tsc",
    "test": "jest",
    "commit": "git add . && lint-staged",
    "update": "npx -p npm-check-updates -c 'ncu -t -u'",
    "minor-update": "npx -p npm-check-updates -c 'ncu -t minor -u'",
    "prepare": "husky install",
    "chromatic": "chromatic --project-token=CHROMATIC_PROJECT_TOKEN"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "stylelint --fix",
      "eslint --fix"
    ]
  },
