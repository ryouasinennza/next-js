{
  "name": "my-app",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "next-update": "npm i react@latest react-dom@latest",
    "sb": "start-storybook -p 6006",
    "sb-build": "build-storybook",
    "sb-update": "sb upgrade",
    "lint": "run-p lint:*",
    "lint:style": "stylelint --fix './src/**/*.tsx'",
    "lint:es": "eslint --fix './src/**/*.{ts,tsx}'",
    "lint:ts": "tsc --noEmit",
    "format": "prettier --write 'src/**/*'",
    "test": "jest",
    "check": "npm run lint && npm run format && npm run test",
    "commit": "git add . && lint-staged -v",
    "update": "npm run next-update && npm run sb-update",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "stylelint --fix",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "date-fns": "^2.26.0",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@storybook/addon-actions": "^6.4.16",
    "@storybook/addon-essentials": "^6.4.16",
    "@storybook/addon-links": "^6.4.16",
    "@storybook/react": "^6.4.16",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "16.11.10",
    "@types/react": "17.0.37",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/typescript-estree": "^5.5.0",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^2.0.1",
    "eslint": "^8.10.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-plugin-testing-library": "^5.0.4",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.0",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "4.5.2"
  }
}
