{
  "private": true,
  "name": "nextjs-starter-kit",
  "scripts": {
    "build": "pnpm cli && next build --no-lint",
    "chromatic": "pnpm cli && chromatic --project-token=CHROMATIC_PROJECT_TOKEN",
    "clean": "rm -rf ./.next ./storybook-static ./out ./node_modules ./tsconfig.tsbuildinfo ./pnpm-lock.yaml",
    "cli": "node ./lib/cli/dist/index.js",
    "cli-build": "pnpm -F ./lib/cli build",
    "cli-watch": "pnpm -F ./lib/cli watch",
    "dev": "pnpm cli && next dev",
    "fmt": "prettier --write './src/**/*'",
    "lint": "stylelint --fix './src/**/*.{ts,tsx}' & eslint --fix './src/**/*.{ts,tsx}'",
    "ncu": "npx npm-check-updates -i --format group",
    "ppj": "prettier-package-json --config ./ppj.config.js --write ./{package.json,lib/cli/package.json}",
    "prepare": "pnpm cli-build && pnpm cli && husky install && rm -rf ./node_modules/.cache/storybook",
    "sb": "pnpm cli && storybook dev -p 6006",
    "sb-build": "pnpm cli && storybook build",
    "start": "next start",
    "test": "jest",
    "type": "tsc"
  },
  "dependencies": {
    "next": "13.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "17.4.0",
    "ress": "5.0.2",
    "styled-components": "5.3.10",
    "swr": "2.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@storybook/addon-actions": "7.0.6",
    "@storybook/addon-essentials": "7.0.6",
    "@storybook/addon-interactions": "7.0.6",
    "@storybook/addon-links": "7.0.6",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addons": "7.0.6",
    "@storybook/client-api": "7.0.6",
    "@storybook/jest": "0.1.0",
    "@storybook/nextjs": "7.0.6",
    "@storybook/preview-web": "7.0.6",
    "@storybook/react": "7.0.6",
    "@storybook/testing-library": "0.1.0",
    "@storybook/testing-react": "2.0.0",
    "@svgr/webpack": "7.0.0",
    "@testing-library/dom": "9.2.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "18.15.13",
    "@types/react": "18.0.38",
    "@types/styled-components": "5.1.26",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "@typescript-eslint/typescript-estree": "5.59.0",
    "babel-jest": "29.5.0",
    "babel-loader": "9.1.2",
    "babel-plugin-styled-components": "2.1.1",
    "chromatic": "6.17.3",
    "csstype": "3.1.2",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-storybook": "0.6.11",
    "eslint-plugin-testing-library": "5.10.3",
    "eslint-plugin-typescript-sort-keys": "2.3.0",
    "eslint-plugin-unicorn": "46.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-styled-components": "7.1.1",
    "lint-staged": "13.2.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.23",
    "postcss-styled-syntax": "0.4.0",
    "postcss-syntax": "0.36.2",
    "prettier": "2.8.7",
    "prettier-package-json": "2.8.0",
    "react-is": "18.2.0",
    "react-test-renderer": "18.2.0",
    "require-from-string": "2.0.2",
    "serve": "14.2.0",
    "storybook": "7.0.6",
    "storybook-addon-designs": "6.3.1",
    "stylelint": "15.6.0",
    "stylelint-8-point-grid": "2.2.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recess-order": "4.0.0",
    "stylelint-config-standard": "33.0.0",
    "stylelint-order": "6.0.3",
    "typescript": "5.0.4",
    "webpack": "5.80.0"
  },
  "volta": {
    "node": "18.16.0",
    "pnpm": "8.3.1"
  },
  "engines": {
    "node": "^18.16.0",
    "pnpm": "^8.3.1"
  },
  "browserslist": [
    "last 2 Chrome major versions",
    "last 2 safari major versions"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "singleAttributePerLine": true
  }
}
