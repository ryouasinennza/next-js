{
  "private": true,
  "name": "nextjs-starter-kit",
  "scripts": {
    "build": "next build --no-lint",
    "chromatic": "chromatic --project-token=CHROMATIC_PROJECT_TOKEN",
    "clean": "rm -rf ./.next ./storybook-static ./out ./node_modules ./tsconfig.tsbuildinfo ./pnpm-lock.yaml",
    "codegen": "node -r esbuild-register ./src/graphql/codegen/run.ts",
    "codegen-dev": "chokidar './src/graphql/{graphqlClient,hooks,resolvers,schema,codegen.ts,commonType.ts,createGraphql.ts}/**/*' --command 'pnpm codegen'",
    "dev": "pnpm svg-component-gen-dev & next dev & pnpm codegen-dev",
    "e2e": "playwright test",
    "e2eui": "playwright test --ui",
    "fmt": "prettier --write './src/**/*'",
    "lint": "stylelint --fix './src/**/*.{ts,tsx}' & eslint --fix ./src --ext .ts,.tsx",
    "ncu": "npx npm-check-updates -i --format group",
    "ppj": "prettier-package-json --config ./ppj.config.js --write ./package.json",
    "prepare": "is-ci || husky && rm -rf ./node_modules/.cache/storybook && pnpm codegen",
    "sb": "pnpm svg-component-gen-dev & storybook dev -p 6006",
    "sb-build": "storybook build",
    "start": "next start",
    "svg-component-gen": "node -r esbuild-register ./svg-component-gen.ts",
    "svg-component-gen-dev": "chokidar './public/svgs' -c 'pnpm svg-component-gen'",
    "test": "vitest",
    "type": "tsc",
    "update-check": "pnpm lint && pnpm fmt && pnpm test && pnpm type && storybook build && next build --no-lint"
  },
  "dependencies": {
    "@chakra-ui/next-js": "2.2.0",
    "@chakra-ui/react": "2.8.2",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@graphql-tools/load-files": "7.0.0",
    "@graphql-tools/merge": "9.0.1",
    "@graphql-tools/resolvers-composition": "7.0.0",
    "framer-motion": "11.0.3",
    "graphql": "16.8.1",
    "graphql-yoga": "5.1.1",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.50.0",
    "react-use": "17.5.0",
    "swr": "2.2.4"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@graphql-typed-document-node/core": "3.2.0",
    "@playwright/test": "1.41.2",
    "@storybook/addon-actions": "7.6.12",
    "@storybook/addon-essentials": "7.6.12",
    "@storybook/addon-interactions": "7.6.12",
    "@storybook/addon-links": "7.6.12",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addons": "7.6.12",
    "@storybook/client-api": "7.6.12",
    "@storybook/client-logger": "7.6.12",
    "@storybook/instrumenter": "7.6.12",
    "@storybook/jest": "0.2.3",
    "@storybook/nextjs": "7.6.12",
    "@storybook/node-logger": "7.6.12",
    "@storybook/preview-api": "7.6.12",
    "@storybook/preview-web": "7.6.12",
    "@storybook/react": "7.6.12",
    "@storybook/testing-library": "0.2.2",
    "@storybook/testing-react": "2.0.0",
    "@storybook/types": "7.6.12",
    "@svgr/webpack": "8.1.0",
    "@testing-library/dom": "9.3.4",
    "@testing-library/jest-dom": "6.4.1",
    "@testing-library/react": "14.2.1",
    "@testing-library/user-event": "14.5.2",
    "@types/cli-color": "2.0.6",
    "@types/fs-extra": "11.0.4",
    "@types/inquirer": "9.0.7",
    "@types/jest": "29.5.12",
    "@types/jsdom": "21.1.6",
    "@types/node": "20.11.16",
    "@types/react": "18.2.51",
    "@types/testing-library__jest-dom": "5.14.9",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "@typescript-eslint/typescript-estree": "6.20.0",
    "@vitejs/plugin-react": "4.2.1",
    "app-root-path": "3.1.0",
    "babel-jest": "29.7.0",
    "babel-loader": "9.1.3",
    "chokidar": "3.5.3",
    "chokidar-cli": "3.0.0",
    "chromatic": "10.7.1",
    "cli-color": "2.0.3",
    "cross-fetch": "4.0.0",
    "css-loader": "6.10.0",
    "csstype": "3.1.3",
    "directory-tree": "3.5.1",
    "esbuild": "0.20.0",
    "esbuild-register": "3.5.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "27.6.3",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-sort-exports": "0.9.1",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-storybook": "0.6.15",
    "eslint-plugin-testing-library": "6.2.0",
    "eslint-plugin-typescript-sort-keys": "3.1.0",
    "eslint-plugin-unicorn": "50.0.1",
    "eslint-plugin-unused-imports": "3.0.0",
    "fs-extra": "11.2.0",
    "husky": "9.0.10",
    "inquirer": "9.2.13",
    "inquirer-file-tree-selection-prompt": "2.0.5",
    "is-ci": "3.0.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.2.1",
    "msw": "2.1.5",
    "postcss": "8.4.33",
    "postcss-styled-syntax": "0.6.4",
    "postcss-syntax": "0.36.2",
    "prettier": "3.2.4",
    "prettier-package-json": "2.8.0",
    "react-is": "18.2.0",
    "react-test-renderer": "18.2.0",
    "require-from-string": "2.0.2",
    "storybook": "7.6.12",
    "storybook-addon-designs": "6.3.1",
    "stylelint": "16.2.1",
    "stylelint-8-point-grid": "2.2.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recess-order": "4.4.0",
    "stylelint-config-standard": "36.0.0",
    "stylelint-order": "6.0.4",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "undici": "6.6.0",
    "vitest": "1.2.2",
    "webpack": "5.90.1"
  },
  "volta": {
    "node": "20.11.0",
    "pnpm": "8.15.1"
  },
  "engines": {
    "node": "^20.11.0",
    "pnpm": "^8.15.1"
  },
  "browserslist": [
    "last 2 Chrome major versions",
    "last 2 safari major versions"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --write",
      "bash -c 'pnpm type'"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "singleAttributePerLine": true
  }
}
