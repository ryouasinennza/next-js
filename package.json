{
  "name": "my-app",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "syntax": "npm-run-all syntax:format syntax:lint:*",
    "syntax:format": "prettier --write 'src/**/*'",
    "syntax:lint:css": "stylelint --fix './src/**/*.tsx'",
    "syntax:lint:js": "eslint './src/**/*.{ts,tsx}'",
