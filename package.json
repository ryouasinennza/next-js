{
  "private": true,
  "name": "nextjs-starter-kit",
  "scripts": {
    "build": "next build --no-lint",
    "chromatic": "chromatic --project-token=CHROMATIC_PROJECT_TOKEN",
    "clean": "rm -rf ./.next ./storybook-static ./out ./node_modules ./tsconfig.tsbuildinfo ./pnpm-lock.yaml",
    "dev": "pnpm svg-component-gen-dev & next dev",
    "fmt": "prettier --write './src/**/*'",
    "lint": "stylelint --fix './src/**/*.{ts,tsx}' & eslint --fix ./src --ext .ts,.tsx",
    "ncu": "npx npm-check-updates -i --format group",
    "ppj": "prettier-package-json --config ./ppj.config.js --write ./package.json",
    "prepare": "husky install && rm -rf ./node_modules/.cache/storybook",
    "sb": "pnpm svg-component-gen-dev & storybook dev -p 6006",
    "sb-build": "storybook build",
    "start": "next start",
    "svg-component-gen": "node -r esbuild-register ./svg-component-gen.ts",
    "svg-component-gen-dev": "chokidar './public/svgs' -c 'pnpm svg-component-gen'",
    "test": "jest",
    "type": "tsc",
    "update-check": "prettier --write './src/**/*' && jest && tsc && storybook build && next build --no-lint"
  },
  "dependencies": {
    "next": "13.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "17.4.0",
    "ress": "5.0.2",
    "styled-components": "5.3.10",
    "swr": "2.1.5"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@storybook/addon-actions": "7.0.12",
    "@storybook/addon-essentials": "7.0.12",
    "@storybook/addon-interactions": "7.0.12",
    "@storybook/addon-links": "7.0.12",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addons": "7.0.12",
    "@storybook/client-api": "7.0.12",
    "@storybook/instrumenter": "7.0.12",
    "@storybook/jest": "0.1.0",
    "@storybook/nextjs": "7.0.12",
    "@storybook/node-logger": "7.0.12",
    "@storybook/preview-web": "7.0.12",
    "@storybook/react": "7.0.12",
    "@storybook/testing-library": "0.1.0",
    "@storybook/testing-react": "2.0.0",
    "@storybook/types": "7.0.12",
    "@svgr/webpack": "8.0.1",
    "@testing-library/dom": "9.3.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/cli-color": "2.0.2",
    "@types/fs-extra": "11.0.1",
    "@types/inquirer": "9.0.3",
    "@types/node": "20.2.1",
    "@types/react": "18.2.6",
    "@types/styled-components": "5.1.26",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "@typescript-eslint/typescript-estree": "5.59.6",
    "@vercel/ncc": "0.36.1",
    "app-root-path": "3.1.0",
    "babel-jest": "29.5.0",
    "babel-loader": "9.1.2",
    "babel-plugin-styled-components": "2.1.3",
    "chokidar": "3.5.3",
    "chokidar-cli": "3.0.0",
    "chromatic": "6.17.4",
    "cli-color": "2.0.3",
    "css-loader": "6.7.4",
    "csstype": "3.1.2",
    "directory-tree": "3.5.1",
    "esbuild": "0.17.19",
    "esbuild-register": "3.4.2",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.3",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-storybook": "0.6.12",
    "eslint-plugin-testing-library": "5.11.0",
    "eslint-plugin-typescript-sort-keys": "2.3.0",
    "eslint-plugin-unicorn": "47.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "fs-extra": "11.1.1",
    "husky": "8.0.3",
    "inquirer": "9.2.5",
    "inquirer-file-tree-selection-prompt": "2.0.5",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-styled-components": "7.1.1",
    "lint-staged": "13.2.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.23",
    "postcss-styled-syntax": "0.4.0",
    "postcss-syntax": "0.36.2",
    "prettier": "2.8.8",
    "prettier-package-json": "2.8.0",
    "react-is": "18.2.0",
    "react-test-renderer": "18.2.0",
    "require-from-string": "2.0.2",
    "serve": "14.2.0",
    "storybook": "7.0.12",
    "storybook-addon-designs": "6.3.1",
    "stylelint": "15.6.2",
    "stylelint-8-point-grid": "2.2.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recess-order": "4.0.0",
    "stylelint-config-standard": "33.0.0",
    "stylelint-order": "6.0.3",
    "ts-node": "10.9.1",
    "tslib": "2.5.2",
    "typescript": "5.0.4",
    "webpack": "5.83.1"
  },
  "volta": {
    "node": "18.16.0",
    "pnpm": "8.5.1"
  },
  "engines": {
    "node": "^18.16.0",
    "pnpm": "^8.5.1"
  },
  "browserslist": [
    "last 2 Chrome major versions",
    "last 2 safari major versions"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "singleAttributePerLine": true
  }
}
